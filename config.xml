<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tiktuk.myapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Tik-tuk</name>
    <description>Tik Tuk - the best modern video sharing platform</description>
    <author email="a.kuchmiy19@gmail.com">Anton Kuchmii</author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="*" />
    <access origin="cdvfile://*"/>
    <access origin="file:///*"/>
    <platform name="android">
        <preference name="AndroidPersistentFileLocation" value="Internal" />
        <preference name="AndroidExtraFilesystems" value="files, assets" />
    </platform>
</widget>
